{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Root",
  "type": "object",
  "required": [
    "id",
    "experimentID",
    "type",
    "outputCount",
    "name",
    "date",
    "filename"
  ],
  "properties": {
    "id": {
      "$id": "#root/id",
      "title": "Id",
      "type": "integer",
      "description": "Experiment result unique identifier",
      "minimum": 0
    },
    "experimentID": {
      "$id": "#root/experimentID",
      "title": "Experimentid",
      "type": "integer",
      "description": "Foreign ID of experiment",
      "minimum": 0
    },
    "type": {
      "$id": "#root/type",
      "title": "Type",
      "type": "integer",
      "description": "Type of experiment",
      "minimum": 1,
      "maximum": 5
    },
    "outputCount": {
      "$id": "#root/outputCount",
      "title": "Outputcount",
      "type": "integer",
      "description": "Number of used output count",
      "minimum": 1,
      "maximum": $TOTAL_OUTPUT_COUNT$
    },
    "name": {
      "$id": "#root/name",
      "title": "Name",
      "description": "Experiment result name",
      "oneOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ]
    },
    "date": {
      "$id": "#root/date",
      "title": "Date",
      "type": "integer",
      "description": "Unix timestamp of date when experiment was played"
    },
    "filename": {
      "$id": "#root/filename",
      "title": "Filename",
      "type": "string",
      "description": "Path to result data of the experiment"
    }
  }
}
