{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/UsedOutputs",
  "definitions": {
    "UsedOutputs": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "led": {
          "type": "boolean"
        },
        "audio": {
          "type": "boolean"
        },
        "image": {
          "type": "boolean"
        },
        "audioFile": {
          "anyOf": [
            {
              "type": "null"
            },
            {
              "type": "string"
            }
          ]
        },
        "imageFile": {
          "anyOf": [
            {
              "type": "null"
            },
            {
              "type": "string"
            }
          ]
        }
      },
      "oneOf": [
        {
          "required": ["led"]
        },
        {
          "required": ["audio", "audioFile"]
        },
        {
          "required": ["image", "imageFile"]
        }
      ],
      "title": "UsedOutputs"
    }
  }
}
