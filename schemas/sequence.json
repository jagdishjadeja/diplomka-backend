{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Root",
  "type": "object",
  "required": [
    "id",
    "experimentId",
    "name",
    "created",
    "data",
    "size",
    "tags"
  ],
  "properties": {
    "id": {
      "$id": "#root/id",
      "title": "Id",
      "type": "integer",
      "description": "Sequence unique identifier",
      "minimum": 0
    },
    "experimentId": {
      "$id": "#root/experimentId",
      "title": "Experimentid",
      "type": "integer",
      "description": "Foreign ID of experiment",
      "minimum": 0
    },
    "name": {
      "$id": "#root/name",
      "title": "Name",
      "type": "string",
      "description": "Sequence name",
      "minLength": 1
    },
    "created": {
      "$id": "#root/created",
      "title": "Created",
      "type": "integer",
      "description": "Unix timestamp of date when experiment was created"
    },
    "data": {
      "$id": "#root/data",
      "title": "Data",
      "type": "array",
      "minItems": 1,
      "items":{
        "$id": "#root/data/items",
        "title": "Items",
        "type": "integer",
        "minimum": 0,
        "maximum": 8
      }
    },
    "size": {
      "$id": "#root/size",
      "title": "Size",
      "type": "integer",
      "minimum": 1
    },
    "tags": {
      "$id": "#root/tags",
      "title": "Tags",
      "type": "array",
      "uniqueItems": true,
      "minItems": 0,
      "items":{
        "$id": "#root/tags/items",
        "title": "Items",
        "type": "string",
        "minLength": 1
      }
    }
  }
}
