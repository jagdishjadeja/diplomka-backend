    "@nestjs/typeorm": "^7.1.0",
    "@nestjs/websockets": "^7.1.3",
    "@stechy1/diplomka-share": "^1.0.25",
    "is-ci": "^2.0.0",
    "jsonschema": "^1.2.6",
    "reflect-metadata": "^0.1.13",
