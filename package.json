    "@nestjs/testing": "^7.1.3",
    "@nrwl/eslint-plugin-nx": "9.4.0",
    "@nrwl/jest": "^9.4.0",
    "@nrwl/nest": "^9.4.0",
    "@nrwl/node": "9.4.0",
    "@nrwl/workspace": "9.4.0",
    "@types/express": "^4.17.6",
    "@types/is-ci": "^2.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.13.1",
